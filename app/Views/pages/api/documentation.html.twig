<script>
document.addEventListener('DOMContentLoaded', function() {
    // Mapeamento de categorias para endpoints e seus nomes
    const categoryEndpoints = {
        'authentication': { endpoints: [], name: 'Autenticação' },
        'condominiums': { endpoints: ['endpoint-condominiums-list', 'endpoint-condominiums-show'], name: 'Condomínios', links: ['Listar', 'Detalhes'] },
        'fractions': { endpoints: ['endpoint-fractions-list', 'endpoint-fractions-show'], name: 'Frações', links: ['Listar', 'Detalhes'] },
        'fees': { endpoints: ['endpoint-fees-list', 'endpoint-fees-fraction'], name: 'Quotas', links: ['Listar (Condomínio)', 'Listar (Fração)'] },
        'payments': { endpoints: ['endpoint-fee-payments-list', 'endpoint-fee-payments-show'], name: 'Pagamentos', links: ['Listar', 'Detalhes'] },
        'expenses': { endpoints: ['endpoint-expenses-list', 'endpoint-expenses-show'], name: 'Despesas', links: ['Listar', 'Detalhes'] },
        'revenues': { endpoints: ['endpoint-revenues-list', 'endpoint-revenues-show'], name: 'Receitas', links: ['Listar', 'Detalhes'] },
        'budgets': { endpoints: ['endpoint-budgets-list', 'endpoint-budgets-show', 'endpoint-budgets-items'], name: 'Orçamentos', links: ['Listar', 'Detalhes', 'Itens'] },
        'bank-accounts': { endpoints: ['endpoint-bank-accounts-list', 'endpoint-bank-accounts-show'], name: 'Contas Bancárias', links: ['Listar', 'Detalhes'] },
        'transactions': { endpoints: ['endpoint-transactions-list', 'endpoint-transactions-show'], name: 'Transações', links: ['Listar', 'Detalhes'] },
        'documents': { endpoints: ['endpoint-documents-list', 'endpoint-documents-show'], name: 'Documentos', links: ['Listar', 'Detalhes'] },
        'occurrences': { endpoints: ['endpoint-occurrences-list', 'endpoint-occurrences-show'], name: 'Ocorrências', links: ['Listar', 'Detalhes'] },
        'assemblies': { endpoints: ['endpoint-assemblies-list', 'endpoint-assemblies-show'], name: 'Assembleias', links: ['Listar', 'Detalhes'] },
        'reservations': { endpoints: ['endpoint-reservations-list', 'endpoint-reservations-show'], name: 'Reservas', links: ['Listar', 'Detalhes'] },
        'spaces': { endpoints: ['endpoint-spaces-list', 'endpoint-spaces-show'], name: 'Espaços', links: ['Listar', 'Detalhes'] },
        'suppliers': { endpoints: ['endpoint-suppliers-list', 'endpoint-suppliers-show'], name: 'Fornecedores', links: ['Listar', 'Detalhes'] },
        'contracts': { endpoints: ['endpoint-contracts-list', 'endpoint-contracts-show'], name: 'Contratos', links: ['Listar', 'Detalhes'] },
        'messages': { endpoints: ['endpoint-messages-list', 'endpoint-messages-show'], name: 'Mensagens', links: ['Listar', 'Detalhes'] },
        'general-info': { endpoints: ['general-info'], name: 'Informações Gerais', links: [] }
    };

    // Função para ocultar todos os cards (exceto autenticação)
    function hideAllCards() {
        // Limpar tabs anteriores
        const tabsList = document.getElementById('endpointsTabs');
        const tabsContent = document.getElementById('endpointsTabContent');
        if (tabsList) tabsList.innerHTML = '';
        if (tabsContent) tabsContent.innerHTML = '';
        
        // Ocultar todos os cards de endpoint
        document.querySelectorAll('.endpoint-card').forEach(card => {
            card.classList.add('d-none');
        });
        
        // Ocultar card de autenticação também quando selecionar outra categoria
        const authCard = document.getElementById('authentication');
        if (authCard) {
            authCard.classList.add('d-none');
        }
    }

    // Função para mostrar cards de uma categoria
    function showCategoryCards(category) {
        hideAllCards();
        
        // Ocultar tabs container
        const tabsContainer = document.getElementById('endpointsTabsContainer');
        if (tabsContainer) {
            tabsContainer.classList.add('d-none');
        }
        
        if (category === 'authentication') {
            const authCard = document.getElementById('authentication');
            if (authCard) {
                authCard.classList.remove('d-none');
                setTimeout(() => {
                    authCard.scrollIntoView({ behavior: 'smooth', block: 'start' });
                }, 100);
            }
            document.getElementById('endpointLinks').classList.add('d-none');
            return;
        }
        
        const categoryData = categoryEndpoints[category];
        if (categoryData && categoryData.endpoints) {
            // Ocultar links dos endpoints (não precisamos mais deles)
            document.getElementById('endpointLinks').classList.add('d-none');
            
            // Se houver apenas um endpoint, mostrar diretamente sem tabs
            if (categoryData.endpoints.length === 1) {
                const endpointId = categoryData.endpoints[0];
                const card = document.getElementById(endpointId);
                if (card) {
                    card.classList.remove('d-none');
                    setTimeout(() => {
                        card.scrollIntoView({ behavior: 'smooth', block: 'start' });
                    }, 100);
                }
                return;
            }
            
            // Criar tabs para múltiplos endpoints
            const tabsList = document.getElementById('endpointsTabs');
            const tabsContent = document.getElementById('endpointsTabContent');
            
            if (tabsList && tabsContent && categoryData.endpoints.length > 1) {
                // Limpar tabs anteriores
                tabsList.innerHTML = '';
                tabsContent.innerHTML = '';
                
                // Criar tabs e tab panes
                categoryData.endpoints.forEach((endpointId, index) => {
                    const card = document.getElementById(endpointId);
                    if (card) {
                        // Criar tab button
                        const tabButton = document.createElement('li');
                        tabButton.className = 'nav-item';
                        tabButton.setAttribute('role', 'presentation');
                        
                        const tabLink = document.createElement('button');
                        tabLink.className = index === 0 ? 'nav-link active' : 'nav-link';
                        tabLink.id = `tab-${endpointId}`;
                        tabLink.setAttribute('data-bs-toggle', 'tab');
                        tabLink.setAttribute('data-bs-target', `#pane-${endpointId}`);
                        tabLink.setAttribute('type', 'button');
                        tabLink.setAttribute('role', 'tab');
                        tabLink.setAttribute('aria-controls', `pane-${endpointId}`);
                        tabLink.setAttribute('aria-selected', index === 0 ? 'true' : 'false');
                        
                        // Usar o título original (links) como prioridade
                        const tabTitle = categoryData.links[index] || (() => {
                            // Fallback: obter título do card se não houver link definido
                            const cardHeader = card.querySelector('.card-header h5');
                            return cardHeader ? cardHeader.textContent.trim() : `Endpoint ${index + 1}`;
                        })();
                        tabLink.textContent = tabTitle;
                        
                        tabButton.appendChild(tabLink);
                        tabsList.appendChild(tabButton);
                        
                        // Criar tab pane
                        const tabPane = document.createElement('div');
                        tabPane.className = index === 0 ? 'tab-pane fade show active' : 'tab-pane fade';
                        tabPane.id = `pane-${endpointId}`;
                        tabPane.setAttribute('role', 'tabpanel');
                        tabPane.setAttribute('aria-labelledby', `tab-${endpointId}`);
                        
                        // Clonar o card completo para o tab pane
                        const cardClone = card.cloneNode(true);
                        cardClone.classList.remove('d-none');
                        cardClone.classList.add('mb-0');
                        cardClone.id = `${endpointId}-tab-clone`; // ID diferente para evitar conflitos
                        tabPane.appendChild(cardClone);
                        
                        // Manter o card original oculto
                        card.classList.add('d-none');
                        
                        tabsContent.appendChild(tabPane);
                    }
                });
                
                // Mostrar container de tabs
                tabsContainer.classList.remove('d-none');
                
                // Scroll para as tabs
                setTimeout(() => {
                    tabsContainer.scrollIntoView({ behavior: 'smooth', block: 'start' });
                }, 100);
            }
        }
    }

    // Função para mostrar um endpoint específico
    function showEndpoint(endpointId) {
        hideAllCards();
        const card = document.getElementById(endpointId);
        if (card) {
            card.classList.remove('d-none');
            card.scrollIntoView({ behavior: 'smooth', block: 'start' });
        }
    }

    // Event listener para cliques no dropdown de categorias
    document.querySelectorAll('.category-link').forEach(link => {
        link.addEventListener('click', function(e) {
            e.preventDefault();
            const category = this.getAttribute('data-category');
            const categoryData = categoryEndpoints[category];
            if (categoryData) {
                document.getElementById('selectedCategory').textContent = categoryData.name;
                showCategoryCards(category);
            }
        });
    });

    // Mostrar autenticação por padrão
    const authCard = document.getElementById('authentication');
    if (authCard) {
        authCard.classList.remove('d-none');
    }
});
</script>
